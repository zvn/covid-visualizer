{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "run-dist": "node ./dist/bin/www",
    "transpile": "babel ./backend --out-dir dist",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development npm-run-all build run-dist",
    "prod": "NODE_ENV=production npm-run-all build run-dist",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "cookie-parser": "~1.4.4",
    "csv-parse": "^4.15.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "backend/*",
      "public/*"
    ],
    "ignore": []
  }
}
